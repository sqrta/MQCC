qreg q[20];

module bcnot(q1,q2){
    choice ([0,1]){
        0:  cnot(q1,q2);

        1:  barrier(q1,q2);
            cnot(q1,q2);
    }
}

module swap(q1,q2){
    bcnot(q1,q2);
    bcnot(q2,q1);
    bcnot(q1,q2);
}

x(q[12]);
x(q[2]);
swap(q[12],q[7]);
swap(q[7] ,q[6]);
swap(q[2], q[1]);
swap(q[6], q[1]);