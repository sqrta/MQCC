qreg q[10];
creg r[1];
fcho c1,c2,c3,c4 = {0, 1, 2};

module twocnot(q0,q1,q2){
    choice ({0,1}) {
        0:  cx(q1,q0);
            cx(q2,q0);
        1:  cx(q2,q0);
            cx(q1,q0);
    }    
}

module h3(q0,q1,q2){
        x(q0);
        x(q1);
        x(q2);
        twocnot(q0,q1,q2);
        rz(3.14/2, q0);
        twocnot(q0,q1,q2);
        x(q0);
        x(q1);
        x(q2); 
}

module h4(q0,q1,q2,q3){
        x(q0);
        x(q1);
        x(q2);
        x(q3);
        twocnot(q0,q1,q2);
        cnot(q3,q0);
        rz(3.14/4, q0);
        cnot(q3,q0);
        twocnot(q0,q1,q2); 
        x(q0);
        x(q1);
        x(q2);
        x(q3);
}

module bv4(q0,q1,q2,q3){
        h(q0);
        h(q1);
        h(q2);
        h(q3);
        twocnot(q0,q1,q2);
        cnot(q3,q0);
        h(q0);
        h(q1);
        h(q2);
        h(q3);
}

choice (c1){
    0:  h3(q[1], q[0], q[3]);

    1:  h3(q[7], q[6], q[8]);

    2:  h3(q[16], q[11], q[17]);
}

choice (c2){
    0:  h4(q[1], q[0], q[3], q[4]);

    1:  h4(q[7], q[6], q[8], q[12]);

    2:  h4(q[16], q[11], q[17], q[15]);
}

choice (c3){
    0:  bv4(q[1], q[0], q[3], q[4]);

    1:  bv4(q[7], q[6], q[8], q[12]);

    2:  bv4(q[16], q[11], q[17], q[15]);
}