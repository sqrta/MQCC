qreg q[5];
fcon c1 = {0, 1};
fcon c2 = {0, 1};

h(q[3]);
h(q[2]);
h(q[1]);
h(q[0]);
control (c1){
    0:  cx(q[3],q[0]);
        cx(q[2],q[0]);
        cx(q[1],q[0]);
        rz(q[0]);
        cx(q[1],q[0]);
        cx(q[2],q[0]);
        cx(q[3],q[0]);
    
    1:  cx(q[3],q[2]);
        cx(q[1],q[0]);
        cx(q[2],q[0]);    
        rz(2.6,q[0]);
        cx(q[2],q[0]);
        cx(q[1],q[0]);
        cx(q[3],q[2]);
}
h(q[3]);
h(q[2]);
h(q[1]);
h(q[0]);

rx(-1.570796e+00, q[3]);
rx(-1.570796e+00, q[2]);
rx(-1.570796e+00, q[1]);
rx(-1.570796e+00, q[0]);

control (c2){
    0:  cx(q[3],q[0]);
        cx(q[2],q[0]);
        cx(q[1],q[0]);
        rz(2.6,q[0]);
        cx(q[1],q[0]);
        cx(q[2],q[0]);
        cx(q[3],q[0]);
    
    1:  cx(q[3],q[2]);
        cx(q[1],q[0]);
        cx(q[2],q[0]);    
        rz(q[0]);
        cx(q[2],q[0]);
        cx(q[1],q[0]);
        cx(q[3],q[2]);
}

rx(1.570796e+00, q[3]);
rx(1.570796e+00, q[2]);
rx(1.570796e+00, q[1]);
rx(1.570796e+00, q[0]);