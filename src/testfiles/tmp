qreg q[10];
creg r[1];
fcho c1,c3 = {0, 1};
fcho c2 = [0,1];

module Bell1(q0, q1, q2){
    h(q0);
    measure(q0,r[0]);
    case (r[0]){
            1:  x(q1);
            0:  pass
        }
        h(q1);
        cnot(q1,q2);
}

module Bell2(q1,q2){
    h(q1);
    cnot(q1, q2);
}


Bell2(q[1], q[2]);
Bell1(q[0], q[1], q[2]);
